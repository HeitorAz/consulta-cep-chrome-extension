{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"baseUrl": ".",
		"paths": {
			"$src": ["src"],
			"$src/*": ["src/*"],
			"$lib":["src/lib"],
			"$lib/*":["src/lib/*"],
			"$providers": ["src/providers"],
			"$providers/*": ["src/providers/*"],
			"$models": ["src/lib/models"],
			"$models/*": ["src/lib/models/*"],
			"$widgets": ["src/lib/widgets"],
			"$widgets/*": ["src/lib/widgets/*"],
		}
	},
}
